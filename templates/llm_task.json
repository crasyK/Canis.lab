{
    "custom_id": "task-{index}",
    "method": "POST",
    "url": "/v1/chat/completions",
    "body": {
        "model": "gpt-4o-mini",
        "temperature": 0.7,
        "messages": [
            {"role": "system", "content": "{systemprompt}"},
            {"role": "user", "content": "{prompt}"}
        ]
    }
}